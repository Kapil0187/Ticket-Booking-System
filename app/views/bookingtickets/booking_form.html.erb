<%= render :partial => 'comman/nav'%>
<%= form_with url: "/bookingtickets", method: :get do |form| %>
  <%= form.label :first_name, "First Name:" %><br>
  <%= form.text_field :first_name %><br><br>

  <%= form.label :last_name, "Last Name:" %><br>
  <%= form.text_field :last_name %><br><br>

  <%= form.label :age, "age:" %><br>
  <%= form.text_field :age%><br><br>

  <%= form.radio_button :gender, "Male" %>
  <%= form.label :gender_male, "Male" %>
  <%= form.radio_button :gender, "Female" %>
  <%= form.label :gender_female, "Female" %> <br><br>

  <%= form.button "Genrate Ticket",type:"submit"%>
<% end %>